<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.simmon.shopstore.repository.contactRepository">
    <insert id="save" parameterType="contact">
        insert into contact(cname,cemail,originalPrice,discountPirce) values (#{cname},#{cemail},#{originalPrice},#{discountPirce})
    </insert>
</mapper>